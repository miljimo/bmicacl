<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:models="clr-namespace:ViewModels"
                     xmlns:views="clr-namespace:BMIApp"
                     xmlns:views_dashboard="clr-namespace:Views.Dashboards"
                    xmlns:local="clr-namespace:BMIApp.Resources">

    <DataTemplate DataType="{x:Type models:DashBoardModelView}" >
        <views:DashboardPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type models:AddBMIViewModel}" >
        <views_dashboard:AddBMIView/>
    </DataTemplate>

</ResourceDictionary>