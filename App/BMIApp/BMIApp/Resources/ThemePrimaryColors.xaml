    <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="PrimaryColor">#66ccff</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush"    Color="{StaticResource PrimaryColor}"  />

    <Color x:Key="PrimaryLightColor">#b3e6ff</Color>
    <SolidColorBrush x:Key="PrimaryLightColorBrush" Color="{StaticResource PrimaryLightColor}" />

    <Color x:Key="PrimaryDarkColor">#1ab2ff</Color>
    <SolidColorBrush x:Key="PrimaryDarkColorBrush" Color="{StaticResource PrimaryDarkColor}" />

    <Color x:Key="PrimaryTextColor" >#ffffff</Color>
    <SolidColorBrush x:Key="PrimaryTextColorBrush"  Color="{StaticResource PrimaryTextColor}" />


    <Color x:Key="TransparentDialogPrimaryDarkColor">#00111a</Color>
    <SolidColorBrush x:Key="TransparentDialogPrimaryDarkColorrBrush"  
                     Color="{StaticResource TransparentDialogPrimaryDarkColor}"  Opacity="0.2" />

</ResourceDictionary>

