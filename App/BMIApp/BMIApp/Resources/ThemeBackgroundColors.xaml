    <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="BackgroundColor">#F7F9F9</Color>
    <SolidColorBrush x:Key="BackgroundColorBrush"
                     Color="{StaticResource BackgroundColor}" />
    <!-- Background Light-->

    <Color x:Key="BackgroundLightColor">#fefefe</Color>
    <SolidColorBrush x:Key="BackgroundLightColorBrush"
                     Color="{StaticResource BackgroundLightColor}" />

    <Color x:Key="BackgroundDarkColor">#F5F8F9</Color>
    <SolidColorBrush x:Key="BackgroundDarkColorBrush"
                     Color="{StaticResource BackgroundDarkColor}" />

    <Color x:Key="BackgroundTextColor" >#161d1d</Color>
    <SolidColorBrush x:Key="BackgroundTextColorBrush"     Color="{StaticResource BackgroundTextColor}" />


    <Color x:Key="DashboardInnerBackgroundPaneColor">#c6b9c4</Color>
    <SolidColorBrush x:Key="DashboardInnerBackgroundPaneColorBrush"  
                     Color="{StaticResource DashboardInnerBackgroundPaneColor}"  />

</ResourceDictionary>

