<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:models="clr-namespace:BMIApplication.ViewModels"
                     xmlns:views="clr-namespace:BMIApplication.Views"
                    xmlns:local="clr-namespace:BMIApplication.Resources">

    <!-- If this does not compile and you know it should, then remove or copy all the data template and compile the app with them,
    if compilation is successful then re-paste all the data template and re-compile 
    Microsoft are not all genuis their system is not without errors-->
    
    <DataTemplate DataType="{x:Type models:DashBoardModelView}" >
        <views:DashboardPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type models:BMICalculatorViewModel}" >
        <views:BMICalculatorView/>
    </DataTemplate>

   

    <DataTemplate DataType="{x:Type models:UserLoginViewModel}" >
        <views:UserLoginView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type models:CreateUserAccountViewModel}">
        <views:UserCreateNewAccountView/>
    </DataTemplate>


</ResourceDictionary>